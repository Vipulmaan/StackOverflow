<%= render "layouts/navbar" %>
<% @page_title = "UserAuth | Edit" %>
<div class="Sign_Form">
  <h1>Edit Profile</h1>
  <%= form_for(@user, url: user_path ) do |f| %>
    <p> Username:</br> <%= f.text_field :name%> </p>
    <p> Email:</br> <%= f.text_field :email%> </p>
    <%= f.submit :Update %>
  <% end %>
</div>